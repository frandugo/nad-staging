{% block html_head_container %}
	{% include 'html-header.twig' %}
	{% block head %}{% endblock %}
</head>{% endblock %}<body class="{{body_class}}" data-template="base.twig">
<a class="skip-link screen-reader-text" href="#content">{{ _e( 'Skip to content') }}</a>

<header class="header">
	{% block header %}
		{% include 'partial/top-bar.twig' %}

		<div class="container">
			<div class="nav__items">
				<nav class="main-menu">
					{{ function('wp_nav_menu', {'theme_location': 'menu-left'}) }}
				</nav>
				<img src="{{theme.link}}'/img/logo.png'" class="hdr__logo-link" href={{site.url}} rel="home">


				<nav class="main-menu">
					{{ function('wp_nav_menu', {'theme_location': 'menu-right'}) }}
				</nav>
			</div>
		</div>
	{% endblock %}
</header>
{% block content %}{% endblock %}
<footer id="footer">
	{% block footer %}
		<div class="container">

			{% include 'footer.twig' %}

		</div>
	{% endblock %}
	{{ function('wp_footer') }}
</footer></body></html>
